<Page x:Class="_11_ResourceDictioanary.Pages.Contact"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:_11_ResourceDictioanary.Pages"
      
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="About">

    <Grid>
        <Button Style="{StaticResource MaterialDesignRaisedDarkButton}" Width="120" Height="50">
            <StackPanel>
                <TextBlock Text="{DynamicResource contact}"/>
            </StackPanel>    
        </Button>
    </Grid>
</Page>
