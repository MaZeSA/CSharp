<Window x:Class="Threads.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Threads"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" Closing="Window_Closing">
    <StackPanel>
        <StackPanel Orientation="Horizontal">
            <Label>Factorial</Label>
            <TextBox Name="txtFactorial"></TextBox>
        </StackPanel>
        <StackPanel Orientation="Horizontal">
            <Label>PrimeNumbers</Label>
            <TextBox Name="txtPrimeNumbers"></TextBox>
        </StackPanel>
        <StackPanel Orientation="Horizontal">
            <Label>Fibonacci</Label>
            <TextBox Name="txtFibonacci"></TextBox>
        </StackPanel>
    </StackPanel>
</Window>
